{
  "manifest_version": 3,
  "name": "Svelte Content Script",
  "version": "0.0.1",
  "action": {
    "default_title": "Popup",
    "default_popup": "index.html"
  },
  "icons": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts/src/main.ts"],
      "media": []
    }
  ]
}
